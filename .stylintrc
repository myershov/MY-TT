{
  "blocks": false,
  "brackets": "never",
  "colons": "never",
  "colors": {
    "expect": "always"
  },
  "exclude": ["node_modules/**/*"],
  "commaSpace": "always",
  "commentSpace": "never",
  "cssLiteral": "never",
  "depthLimit": 4,
  "duplicates": false,
  "efficient": "false",
  "extendPref": false,
  "globalDupe": false,
  "indentPref": 2,
  "leadingZero": "never",
  "maxErrors": false,
  "maxWarnings": false,
  "mixed": true,
  "namingConvention": false,
  "namingConventionStrict": true,
  "none": "never",
  "noImportant": false,
  "parenSpace": "never",
  "placeholders": "always",
  "prefixVarsWithDollar": "always",
  "quotePref": false,
  "semicolons": "never",
  "sortOrder": "grouped",
  "stackedProperties": "never",
  "trailingWhitespace": "false",
  "universal": false,
  "valid": false,
  "zeroUnits": "never",
  "zIndexNormalize": false
}